msgid "Game"
msgstr "Гра"

#: PathsActivity.py:124
msgid "Start a new game."
msgstr "Почати нову гру."

#: PathsActivity.py:128
msgid "Play with the robot."
msgstr "Грати з комп’ютером."

#: PathsActivity.py:137
msgid "Turn complete"
msgstr "Хід завершено"

#: PathsActivity.py:143
msgid "Help"
msgstr "Довідка"

